<script>
    $('#messageform').submit(function (e) {

    e.preventDefault();
    console.log($.ajax)
    var data = $(this).serialize()
    console.log(data)
    $.ajax({
        url: '/vendor/message',
        data: data,
        type: 'POST',
        success: function(elem){

            $('#hi').html(data)
        }
    });
});</script>